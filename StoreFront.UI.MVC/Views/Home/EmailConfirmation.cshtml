@model StoreFront.UI.MVC.Models.ContactViewModel
@{
    ViewBag.Title = "Email Confirmation";
}
<section class="breadcrumb-section set-bg" data-setbg="/Content/img/breadcrumb.jpg">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <div class="breadcrumb__text">
                    <h2>@ViewBag.Title</h2>
                    <div class="breadcrumb__option">
                        <a href="@Url.Action("Index", "Home")">Home</a>
                        <span>@ViewBag.Title</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="text-center" style="padding:45px 0 45px">
    <h4>We received the following message:</h4>
    <p>@Model.Message</p><!--M allows us to call that Model up and add a . to access the object we want.-->
    <h4>We will respond back as soon as possible to @Model.Email. Thank you for contacting us!</h4>
</div>
